{
  "manifest_version": 2,

  "name": "Twitch Subscriber Emotes",
  "description": "This extension lets you use any subscriber emote on Twitch",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "http://twitch.tv/",
    "http://code.jquery.com/",
    "https://twitchemotes.com/",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.twitch.tv/*"],
      "js": ["jquery.js", "script.js"]
    }
  ]
}